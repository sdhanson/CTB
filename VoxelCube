using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class VoxelCube : MonoBehaviour {

	GameObject Voxel = new GameObject();

	// Use this for initialization
	void Start () {
		for (int i = 0; i < 8; i++)
			for (int j = 0; j < 8; j++)
				for (int k = 0; k < 8; k++)
					Voxel = VoxelTools.MakeCube (i, j, k);
	}

}
